import subprocess

def runcmd(cmd, verbose = False, *args, **kwargs):

    process = subprocess.Popen(
        cmd,
        stdout = subprocess.PIPE,
        stderr = subprocess.PIPE,
        text = True,
        shell = True
    )
    std_out, std_err = process.communicate()
    if verbose:
        print(std_out.strip(), std_err)
    pass

runcmd('echo "Hello, World!"', verbose = True)
runcmd("wget https://www.scrapingbee.com/images/logo-small.png", verbose = True)
ww="wget https://dcs.megaphone.fm/TWI6021185329.mp3\?key\=6fd48f265ffb65859031af3bf76c65f4\&request_event_id\=bd71bc80-981d-4b56-9b4f-9a3ecd4614fc -O AskTech.mp3"
runcmd(ww, verbose = True)
