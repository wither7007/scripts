def sets(nums):
    seen = set()
    for n in nums:
        if n in seen:
            continue
        seen.add(n)
    return seen
sets(c)
def dic(nums):
    seen = {}
    for n in nums:
        if n in seen:
            continue
        seen[n] = None
    print(seen)
    return total
dic(c)
def dic(nums):
    seen = {}
    for n in nums:
        if n in seen:
            continue
        seen[n] = None
    print(seen)
    return seen
