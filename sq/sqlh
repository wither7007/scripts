def bulkInsert(records):
    try:
        sql_insert_query = """ INSERT INTO author (name, age) 
                           VALUES (%s,%s) """

        # executemany() to insert multiple rows
        result = cursor.executemany(sql_insert_query, records)
        conn.commit()
        print(cursor.rowcount, "Record inserted successfully into author table")

    except (Exception, psycopg.Error) as error:
        print("Failed inserting record into  table {}".format(error))
        breakpoint()

    finally:
        # closing database connection.
        if conn:
            # cursor.close()
            # conn.close()
            print("PostgreSQL connection is commented")


import json
def sheet(sh):
    url = f"https://sheets.googleapis.com/v4/spreadsheets/1v0WTX_g0SEHb-EfG9faV3ayFo1WZUmUj8Lhgc2Kw2cA/values/{sh}?alt=json&key=AIzaSyCZ3y8Es42zvNGON7ezA6q4dxe8RNcyQIs"
    r=requests.get(url)
    ll=r.json()
    return ll
