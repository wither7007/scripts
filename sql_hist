        schemaSql='''
SELECT sql 
FROM sqlite_master 
'''
        records = cursor.fetchall()


    except sqlite3.Error as error:
        print("Error while working with SQLite", error)
    finally:
        if sqliteConnection:
            sqliteConnection.close()
            print("sqlite connection is closed")
            return records
            
