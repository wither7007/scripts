z
def fb(nums):
    x,y=0,1
    for _ in range(nums):
        x,y=y, x+y
        yield x
        
[a for a in fb(8)]
[a for a in fb(88)]
[a for a in fb(9)]
def fb(nums):
    x,y=0,1
    for _ in range(nums):
        x,y=y, x+y
        yield f'{x:_}'
        
[a for a in fb(9)]
[a for a in fb(30)]
[a for a in fb(83)]
def fb(nums):
    x,y=0,1
    for _ in range(nums):
        x,y=y, x+y
        print(f'{x},{y}=y, x+y')
        yield f'{x:_}'
        
