def smart_divide(func):
    def inner(a, b):
        print("I am going to divide", a, "and", b)
        if b == 0:
            print("Whoops! cannot divide")
            return

        return func(a, b)
    return inner
    
def f(a,b):
    return a/b
    
f(3,2)
new=smart_divide(f)
new(3,2)
new(3,0)
