class Program:
# The instance attributes
   def __init__(self, name, age):
      self.name = name
      self.age = age
# The instance method
   def plant(self, eucalyptus):
      return "{} plants {}".format(self.name, eucalyptus)

   def animals(self):
      return "{} animals".format(self.name)

# instantiating the object
Pen = Program("Pen", 10)
Pen
type(Pen)
vars(Pen)
Pen.plant()
Pen.plant('cactus')
Pen.animals()
