    sys.stdout = sys.__stdout__

print('This message is shown')
try:
    with open('myf', "w", encoding='utf-8') as target:
        sys.stdout = target
        # inspect(cfilter,all=True)
        help(sys)
        print('This gets redirected to nothing')
finally:
    sys.stdout = sys.__stdout__
    
