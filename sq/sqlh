import os
from datetime import datetime
fil=[a.name for a in os.scandir('.') if a.name.endswith('.jpg')]



#for date array
def md(filename):
    t = os.path.getmtime(filename)
    return datetime.fromtimestamp(t).strftime( "%m-%d-%Y %H:%M:%S" )


dates=[]
for a in fil:
    dates.append([a,md(a)])


#for sort array
newd=sorted(dates,key=lambda x: jr(x))
import os
from datetime import datetime
fil=[a.name for a in os.scandir('.') if a.name.endswith('.jpg')]



#for date array
def md(filename):
    t = os.path.getmtime(filename)
    return datetime.fromtimestamp(t).strftime( "%m-%d-%Y %H:%M:%S" )


dates=[]
for a in fil:
    dates.append([a,md(a)])

def jr(x):
    return datetime.strptime(x[1], "%m-%d-%y %H:%M:%S")

#for sort array
newd=sorted(dates,key=lambda x: jr(x))
import os
from datetime import datetime
fil=[a.name for a in os.scandir('.') if a.name.endswith('.jpg')]



#for date array
def md(filename):
    t = os.path.getmtime(filename)
    return datetime.fromtimestamp(t).strftime( "%m-%d-%y %H:%M:%S" )


dates=[]
for a in fil:
    dates.append([a,md(a)])

def jr(x):
    return datetime.strptime(x[1], "%m-%d-%y %H:%M:%S")

#for sort array
newd=sorted(dates,key=lambda x: jr(x))
newd
#!/usr/bin/python3.10
import os
from datetime import datetime
fil=[a.name for a in os.scandir('.') if a.name.endswith('.jpg')]



#for date array
def md(filename):
    t = os.path.getmtime(filename)
    return datetime.fromtimestamp(t).strftime( "%m-%d-%y %H:%M:%S" )


dates=[]
for a in fil:
    dates.append([a,md(a)])

def format_date(x):
    return datetime.strptime(x[1], "%m-%d-%y %H:%M:%S")

#for sort array
newd=sorted(dates,key=lambda x: format_date(x))
print(newd)
print('/n'.join(newd))
[" - ".join(a) for a in newd]
print([" - ".join(a) for a in newd])
print("\n".join([" - ".join(a) for a in newd]))
