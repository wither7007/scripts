def pt(max=0):
    n = 0
    while n < max:
        yield 2 ** n
        n += 1
pt(3)
[a for a in pt(3)]
y,z=1,2
y
def fb(nums):
    x,y=0,1
    for _ in range(nums):
        x,y=y, x+y
        yield x
[a for a in fb(8)]
[a for a in fb(88)]
[a for a in fb(9)]
        yield f'{x:_}'
[a for a in fb(30)]
[a for a in fb(83)]
        print(f'{x},{y}=y, x+y')
[a for a in fb(3)]
        print(f'{x},{y}={y}, {x+y}')
[a for a in fb(5)]
[a for a in fb(10)]
[a for a in fb(20)]
        yield f'{_} is {x:_}'
