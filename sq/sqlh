from pathlib import Path
from datetime import date
from datetime import datetime
from export import cfilter
import importlib
importlib.reload(cfilter)
import re
import os
import glob
from tinytag import TinyTag
# datetime object containing current date and time
mpath="/mnt/c/twit/"
files=cfilter(mpath,5)
files=[a for a in files if ".mp" in a]
for fi in files:
    ti=TinyTag.get(fi)
    # print(f'{fi}   {ti.title}')
    ext=os.path.splitext(fi)[1]
    if ti.title:
        com=re.sub(r'[^a-zA-Z0-9]','_', ti.title)
        com=re.sub(r'_{2,}','_', com)
    else:
        com='NONE'
    newFile=mpath+com+ext
    print(f'os.rename({fi}, {newFile})')
    os.rename(fi,newFile)
    
from pathlib import Path
from datetime import date
from datetime import datetime
# from export import cfilter
import importlib
import re
import os
import glob
from tinytag import TinyTag
# datetime object containing current date and time
mpath="/mnt/c/twit/"
files=cfilter(mpath,5)
files=[a for a in files if ".mp" in a]
for fi in files:
    ti=TinyTag.get(fi)
    # print(f'{fi}   {ti.title}')
    ext=os.path.splitext(fi)[1]
    if ti.title:
        com=re.sub(r'[^a-zA-Z0-9]','_', ti.title)
        com=re.sub(r'_{2,}','_', com)
    else:
        com='NONE'
    newFile=mpath+com+ext
    print(f'os.rename({fi}, {newFile})')
    os.rename(fi,newFile)
    
