print([a for a,b in mysub[1].items()] not b ==False)
print([(a,b) for a,b in mysub[1].items()] not b ==False)
print([(a,b) for a,b in mysub[1].items()] not b ==False])
print([(a,b) for a,b in mysub[1].items() ])
