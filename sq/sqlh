dbName="/mnt/c/projects/Django/first/mysite/db.sqlite3"
def addDeveloper(id, name, joiningDate):
    try:
        sqliteConnection = sqlite3.connect(dbName)
        cursor = sqliteConnection.cursor()
        print("Connected to SQLite")

        # insert developer detail
        sqlite_insert_with_param = """INSERT INTO 'polls_question'
                          ('id', 'question_text', 'pub_date') 
                          VALUES (?, ?, ?);"""

        data_tuple = (id, name, joiningDate)
        cursor.execute(sqlite_insert_with_param, data_tuple)
        sqliteConnection.commit()
        print("Developer added successfully \n")

        # # get developer detail
        # sqlite_select_query = """SELECT id, question_text from polls_question where id = ?"""
        # cursor.execute(sqlite_select_query, (1,))
        # records = cursor.fetchall()

        # for row in records:
        #     developer = row[0]
        #     joining_Date = row[1]
        #     print(developer, " joined on", joiningDate)
        #     print("joining date type is", type(joining_Date))

        cursor.close()

    except sqlite3.Error as error:
        print("Error while working with SQLite", error)
    finally:
        if sqliteConnection:
            sqliteConnection.close()
            print("sqlite connection is closed")

