def chop(x,l=14):
    ch=re.split('\s', x)
    n=0
    pdb.set_trace()
    r=[]
    fl=len(x)
    for a in range(len(ch)):
        r.append(' '.join(ch[n:n+l]))
        n+=l
        if n>fl
             break
    return r
