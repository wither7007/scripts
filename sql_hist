def dec(x):
    fd=x.split(':')
    return int(fd[0])*60+int(fd[1])

songCut=f"{song.replace(ext,'')}_{ss.replace(':','-')}-{to.replace(':','-')}{ext}"
def sl(x):
    ret= f"ffmpeg -i {song} -ss {dec(ss)} -to {dec(to)} -vcodec libx264 -acodec copy {songCut}"
    return ret
    
