import PILfrom PIL
import Imagefrom PIL.ExifTags 
import TAGSdef 

def resize(filepath, filepath_new):

    percent = 50

    img = Image.open(filepath)

    exifdata = img.getexif()

    width  = int( img.size[0] * percent / 100 )

    height = int( img.size[1] * percent / 100 )

    new_size = (width, height)

    img = img.resize(new_size, Image.Resampling.LANCZOS)

    img.save(filepath_new, exif = exifdata)

    modify_time = os.path.getmtime(filepath)

    os.utime(filepath_new, (modify_time, modify_time))
