def red(x='ins(p)'):
    # pdb.set_trace()
    x=str(x)
    f = io.StringIO()
    with redirect_stdout(f):
        print('\n\n')
        # print('xx'*30)
        eval(x)
    woo = f.getvalue()
    woo2=re.sub(r'\x1B\[[0-9;]*[a-zA-Z]','',woo)
    with open('x','w') as f:
        print(woo2,file=f)
    print(woo2)
    return woo2
