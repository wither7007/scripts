    return datetime.fromtimestamp(t).strftime( "%m-%d-%Y %H:%M:%S" )
    # return datetime.datetime.fromtimestamp(t).strftime( "%m-%Y" )
dates=[]
for a in fil:
    dates.append([a,md(a)])
    
