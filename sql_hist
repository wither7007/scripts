def counts(inp):
    dd={key:0 for key in inp}
    for l in inp:
        if l in list(dd.keys()):
            dd[l]=dd[l]+1
def counts(inp):
    dd={key:0 for key in inp}
    for l in inp:
        if l in list(dd.keys()):
            dd[l]=dd[l]+1
    return dd
