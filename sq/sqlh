def red(x='ins(p)'):
    xx=str(x)
    print(xx)
    _=input('wait')
    f = io.StringIO()
    with redirect_stdout(f):
        print('\n\n')
        # print('xx'*30)
        eval(xx)
    woo = f.getvalue()
    # pdb.set_trace()
    woo2=re.sub(r'\x1B\[[0-9;]*[a-zA-Z]','',woo)
    woo3=re.sub(r'\n{2,}',' ',woo2)
    with open('x','w') as f:
        print(woo3,file=f)
    print(f' woo2 is: {woo3}')
    return woo3
