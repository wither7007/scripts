import requests
 
query = 'python'
 
url = 'https://en.wikipedia.org/w/api.php'
params = {
            'action':'query',
            'format':'json',
            'list':'search',
            'utf8':1,
            'srsearch':query
        }
 
data = requests.get(url, params=params).json()
 
for i in data['query']['search']:
    print(i['title'], ' - Word count: ', i['wordcount'])
import requests
 
query = 'nixon'
 
url = 'https://en.wikipedia.org/w/api.php'
params = {
            'action':'query',
            'format':'json',
            'list':'search',
            'utf8':1,
            'srsearch':query
        }
 
data = requests.get(url, params=params).json()
 
for i in data['query']['search']:
    print(i['title'], ' - Word count: ', i['wordcount'])
    
