#for date array
def md(filename):
    ev=f'os.path.get{t}time(filename)'
    mytime = eval(ev)
    return datetime.fromtimestamp(mytime).strftime( "%m-%d-%y %H:%M:%S" )


dates=[]
for a in fil:
    dates.append([a,md(a)])

def format_date(x):
    return datetime.strptime(x[1], "%m-%d-%y %H:%M:%S")

#for sort array
newd=sorted(dates,key=lambda x: format_date(x))
print("\n".join([" - ".join(a) for a in newd]))
