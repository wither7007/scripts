#!/usr/bin/python3.10
def decTime(x):
    r= f'{int(x/60)}:{int(x%60)}'
    return r

def fime(x):
    a,b=x.split(':')
    return int(a)*60+int(b)
import sys

if len(sys.argv) > 1:
    print(f"Hello, {sys.argv[1]}!")
else:
    print("Hello, world!")
