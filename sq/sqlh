import pudb
from pathlib import Path
from datetime import date
from datetime import datetime
from export import cfilter
import importlib
import re
import os
import glob
from tinytag import TinyTag
# datetime object containing current date and time
mpath="/mnt/c/twit/"
files=cfilter(mpath,8)
files=[a for a in files if ".mp" in a]
for fi in files:
    ti=TinyTag.get(fi)
    # print(f'{fi}   {ti.title}')
    ext=os.path.splitext(fi)[1]
    if ti.title:
        pdb.set_trace()
        ti=TinyTag.get(ti)
        com=re.sub(r'[^a-zA-Z0-9]','_', ti.title)
        com=re.sub(r'_{2,}','_', com)
        com2=ti.album
        com3=''.join(re.findall('[A-Z]', ti.album))
        com=com3+'_'+com
        # print(com4)
    else:
        com='NONE'
    newFile=mpath+com+ext
    
import pdb
from pathlib import Path
from datetime import date
from datetime import datetime
from export import cfilter
import importlib
import re
import os
import glob
from tinytag import TinyTag
# datetime object containing current date and time
mpath="/mnt/c/twit/"
files=cfilter(mpath,8)
files=[a for a in files if ".mp" in a]
for fi in files:
    ti=TinyTag.get(fi)
    # print(f'{fi}   {ti.title}')
    ext=os.path.splitext(fi)[1]
    if ti.title:
        pdb.set_trace()
        ti=TinyTag.get(ti)
        com=re.sub(r'[^a-zA-Z0-9]','_', ti.title)
        com=re.sub(r'_{2,}','_', com)
        com2=ti.album
        com3=''.join(re.findall('[A-Z]', ti.album))
        com=com3+'_'+com
        # print(com4)
    else:
        com='NONE'
    newFile=mpath+com+ext
    
from pathlib import Path
from datetime import date
from datetime import datetime
from export import cfilter
import importlib
import re
import os
import glob
from tinytag import TinyTag
# datetime object containing current date and time
mpath="/mnt/c/twit/"
files=cfilter(mpath,8)
files=[a for a in files if ".mp" in a]
for fi in files:
    ti=TinyTag.get(fi)
    # print(f'{fi}   {ti.title}')
    ext=os.path.splitext(fi)[1]
    if ti.title:
        pdb.set_trace()
        ti=TinyTag.get(fi)
        com=re.sub(r'[^a-zA-Z0-9]','_', ti.title)
        com=re.sub(r'_{2,}','_', com)
        com2=ti.album
        com3=''.join(re.findall('[A-Z]', ti.album))
        com=com3+'_'+com
        # print(com4)
    else:
        com='NONE'
    newFile=mpath+com+ext
 
import bs4
from bs4 import BeautifulSoup as soup
from urllib.request import urlopen

news_url="https://news.google.com/news/rss"
Client=urlopen(news_url)
xml_page=Client.read()
Client.close()

soup_page=soup(xml_page,"xml")
news_list=soup_page.findAll("item")
# Print news title, url and publish date
for news in news_list:
  print(news.title.text)
  print(news.link.text)
  print(news.pubDate.text)
  print("-"*60)
  
