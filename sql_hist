class Zz:
    def __init__(self, myDir):
        self.myDir = myDir

    def __repr__(self):
        return f"{self.myDir}"

    def cc(self):
        ll=[a for a in os.scandir(self.myDir)]
        [a for a in subdirs('.')]       
        # print([a for a in os.scandir(self.myDir)]
        return ll

    def subdirs(self):
        """Yield directory names not starting with '.' under given path."""
        for entry in os.scandir(self):
            if entry.is_file():
                yield entry.name

