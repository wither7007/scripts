import re
import sys
import pdb
def winL(x):
    print(x)
    pdb.set_trace()
    y=x.replace(r"C:", '/mnt/c').replace('\\','/')
    print(x.replace(r"C:", '/mnt/c').replace('\\','/'))
    cps(y)
    return y
def pConv(pat='/mnt/'):
    if len(sys.argv)>1:
        aa=sys.argv[1]
    pdb.set_trace()
    patx = re.search("/mnt/c/", pat)
    if patx:
        bb=pat.replace('/mnt/c/','c:\\').replace('/', '\\')
        print(f"{pat} is {bb}")

    else:
        print("not linupatx")
    patx = re.search(r"c:\\", pat)
    if patx:
        print('windows')
        winL(patx)
pConv("c:\path")
