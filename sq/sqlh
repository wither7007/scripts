list(1,3)
def ss(x,y,z):
    xx=list[x,y,z]
    return xx
    
ss(1,3,3)
def ss(x,y,z):
    xx=list([x,y,z])
    return xx
    
ss(1,3,3)
set(ss(1,3,3))
def ss(x,y,z):
    xx=list([x,y,z])
    if len(xx)==len(set(xx)):
        return sum(xx)
    else:
        return 0

