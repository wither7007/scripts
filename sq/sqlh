def gcd(a,b):
    for t in range(30,0,-1):
        if a % t==0 and b % t ==0:
            print(t) 
            
gcd(20,15)
def gcd(a,b):
    c=max(a,b)
    for t in range(c,0,-1):
        if a % t==0 and b % t ==0:
            return t
            
gcd(20,15)
gcd(35,15)
gcd(35,7)
gcd(42,7)
gcd(4200,700)
gcd(4200,710)
