import psycopg
from con import cursor
from con import conn
def myfun(x,y):
    sql=f"insert into author(name,age) VALUES ('{x}','{y}')"
    print(sql)
    try:
        cursor.execute(sql)
        conn.commit()
    except Exception as e:
        print(f'Error {e}')
        print('Anything else that you feel is useful')
        conn.rollback()
myfun('turgenev','52')
import psycopg
from con import cursor
from con import conn
def myfun(x,y):
    sql=f"insert into author(name,age) VALUES ('{x}','{y}')"
    print(sql)
    try:
        cursor.execute(sql)
        conn.commit()
    except Exception as e:
        print(f'Error {e}')
        print('Anything else that you feel is useful')
        conn.rollback()
myfun('turgenev','52')
myfun('turgenev','52')
