def myfun(x,y):
    sql=f"insert into author(name,age) VALUES ('{x}','{y}')"
    print(sql)
    try:
        cursor.execute(sql)
        conn.commit()
    except Exception as e:
        print(f'Error {e}')
        print('Anything else that you feel is useful')
        conn.rollback()
