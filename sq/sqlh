def ns (s):
    regex = r"\s{2,}"
    subst = "\\n"
    s=s.strip().replace('\nPrize\nClaimed\nRemaining\n','') 
    result = re.sub(regex, subst, s, 0, re.MULTILINE)
    length=str(len(result.split('\n'))-2)
    # result=length+'\n'+result+'\n\n---'
    # breakpoint()
    result=result.split('\n')[2:6]
    return result


