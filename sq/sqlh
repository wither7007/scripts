def chop(x,l=14):
    ch=re.split('\s', x)
    n=0
    # pdb.set_trace()
    r=[]
    for a in range(len(ch)):
        r.append(' '.join(ch[n:n+l]))
        n+=l
        if n>200:
            break
    return r

