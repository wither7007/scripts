import os
from datetime import datetime
import argparse
import pudb
parser = argparse.ArgumentParser()

parser.add_argument('-x', type=str, required=False, default='.mp3', help="-x Extension type")
parser.add_argument('-t', type=str, required=False, default='a', help="-t Stat type")

args = parser.parse_args()

x = args.x
t= args.t

fil=[a.name for a in os.scandir('.') if a.name.endswith(x)]

#for date array
def md(filename):
    #accest time is geta
    ev=f'os.path.get{a}time(filename)'
    pudb.set_trace()
    print(ev)
    t = eval(ev)
    return datetime.fromtimestamp(t).strftime( "%m-%d-%y %H:%M:%S" )


dates=[]
for a in fil:
    dates.append([a,md(a)])

def format_date(x):
    return datetime.strptime(x[1], "%m-%d-%y %H:%M:%S")

#for sort array
newd=sorted(dates,key=lambda x: format_date(x))
print("\n".join([" - ".join(a) for a in newd]))
fil
