mult = dict(KB=2**10, MB=2**20, GB=2**30)
mult
def getsize(aline):
  fn, size = aline.split(':', 1)
  value, unit = size.split(' ')
  multiplier = mult[unit]
  return float(value) * multiplier
  
gitsize('10kb')
