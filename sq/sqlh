from datetime import datetime, timedelta, tzinfo
from calendar import timegm
def timeln(x):

    ep = 116444736000000000  # January 1, 1970 as MS file time
    mil = 10000000
    l=(int(x)/10_000_000)-11_644_473_600
    x=int(x)
    mm=((x - ep) / mil)
    print(mm)
    return mm
    
timeln(myt)
myt
def timeln(x):

    ep = 116444736000000000  # January 1, 1970 as MS file time
    ep=11_644_473_600
    mil = 10000000
    l=(int(x)/10_000_000)-11_644_473_600
    x=int(x)
    mm=((x - ep) / mil)
    print(mm)
    return mm
    
timeln(myt)
