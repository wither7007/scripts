get_ipython().run_line_magic('c', '')
def pConv(pat='/mnt/'):
    if len(sys.argv)>1:
        aa=sys.argv[1]
    x = re.search("/mnt/c/", pat)
    if x:
        bb=pat.replace('/mnt/c/','c:\\').replace('/', '\\')
        print(f"{pat} is {bb}")
    else:
        print("not linux")
    x = re.search(r"c:\\", pat)
    if x:
        print('windows')
        winL(x)
