def isprime(number: int) ->bool:
    if number < 2:
        return False
    for element in range(2, int(sqrt(number)+1)):
        if number % element==0;
            return True
    return False
def isprime(number: int) ->bool:
    if number < 2:
        return False
    for element in range(2, int(sqrt(number)+1)):
        if number % element==0:
            return True
    return False
    
