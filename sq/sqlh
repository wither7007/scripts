from pprint import pprint

# create a class
class Student:
    # class variable
    var = 1

    def __init__(self, name, age, id):
        self.name = name
        self.age = age
        self.id = id


# creating object
s = Student("Alice", 14, 23)

# calling the vars() function
pprint(vars())
from pprint import pprint

# create a class
class Student:
    # class variable
    var = 1

    def __init__(self, name, age, id):
        self.name = name
        self.age = age
        self.id = id


