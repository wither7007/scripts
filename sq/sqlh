res
import pathlib

# folder path
dir_path = r'/mnt/c/all'

# to store file names
res = []

# construct path object
d = pathlib.Path(dir_path)

# iterate directory
for entry in d.iterdir():
    # check if it a file
    if entry.is_file():
        res.append(entry)
print(res)
res
res[1]
