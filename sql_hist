def subdirs(dp):
    """Yield directory names not starting with '.' under given path."""
    for entry in os.scandir(dp):
        if entry.is_file():
            yield entry.name
subdirs('.')
[a for a in subdirs('.')]
[a for a in subdirs('/')]
