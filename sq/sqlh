def isprime(number: int) ->bool:
    if number < 2:
        return False
    for element in range(2, int(sqrt(number)+1)):
        if number % element==0;
            return True
    return False
def isprime(number: int) ->bool:
    if number < 2:
        return False
    for element in range(2, int(sqrt(number)+1)):
        if number % element==0:
            return True
    return False
    
isprime(3)
from math import sqrt
isprime(3)
3 % 10
3 % 10==0
def isprime(number: int) ->bool:
    if number < 2:
        return False
    for element in range(2, int(sqrt(number)+1)):
        if number % element==0:
            return False
    return True
isprime(3)
isprime(33)
isprime(31)
def div(a:int, b:int)->float:
    return a/b
    
div(3,3)
div(3,0)
def dec(fun):
    def infun(x,y):
        if y==0:
            print('can\'t divide zero')
        return
    fun(a,b)
    return infun

