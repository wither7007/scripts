def chop(x,l=14):
    ch=re.split('\s', x)
    n=0
    # pdb.set_trace()
    r=[]
    fl=len(ch)
    for a in range(len(ch)):
        # pdb.set_trace()
        r.append(' '.join(ch[n:n+l]))
        n+=l
        if n>fl:
             break
    return r

def ws(x):
    print(x)
    ret=wikipedia.summary(x)
    retc=chop(ret)
    print('\n'.join(retc))
    return retc

def wss(x):
    # print(x)
    res=wikipedia.search(x)
    ee=list(zip([a for a in range(len(res))],res))
    ff=['-'.join(map(str,a)) for a in ee]
    rend(ff)
    jk=int(input('which: '))
    # pdb.set_trace()
    kl=ws(ee[jk][1])

    return ee
ws('haggard')
