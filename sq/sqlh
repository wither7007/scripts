import requests
from bs4 import BeautifulSoup as soup
url="https://www.bbc.co.uk/programmes/p01dh5yg/episodes/downloads"
url="https://twit.tv/shows/this-week-in-enterprise-tech/episodes/532?autostart=false"
print("url is " + url)
page_raw = requests.get(url).text
soup = soup(page_raw, "html.parser")
for script in soup(["script", "style"]):
    script.extract()
bod = str(soup.body)
fileRan='soup.html'
with open(fileRan, "w", encoding="utf-8") as tex:
    print(datetime.now().strftime("%a %m/%d/%Y, %H:%M:%S"), file=tex)
    print(soup.prettify(), file=tex)
    
from datetime import datetime
import requests
from bs4 import BeautifulSoup as soup
url="https://www.bbc.co.uk/programmes/p01dh5yg/episodes/downloads"
url="https://twit.tv/shows/this-week-in-enterprise-tech/episodes/532?autostart=false"
print("url is " + url)
page_raw = requests.get(url).text
soup = soup(page_raw, "html.parser")
for script in soup(["script", "style"]):
    script.extract()
bod = str(soup.body)
fileRan='soup.html'
with open(fileRan, "w", encoding="utf-8") as tex:
    print(datetime.now().strftime("%a %m/%d/%Y, %H:%M:%S"), file=tex)
    print(soup.prettify(), file=tex)
    
