import sqlite3


try:
  conn = sqlite3.connect(dbName, isolation_level=None)
  cursor = conn.cursor()
  print("Database created!")
except Exception as e:
  print("Something bad happened: ", e)
  if conn:
    conn.close()
    
  
  
