clip=dirI+song+".mp4"
start=(6,4)
finish=(6,43)
sts=str( start[0] )+'_'+str( start[1] )
stf=str( finish[0] )+'_'+str( finish[1] )
out=dirO+song+'-'+sts+'-'+stf+'.mp4'
# clipOut=dirO+song+'-'+sts+'-'+stf+'.mp4'
