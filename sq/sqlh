import re

regex = r"\s{2,}"

test_str = ("\n"
	"                \n"
	"                                    \n"
	"                                                    \n"
	"                                                \n"
	"                                \n\n\n"
	"                    $3 -  One Word Crossword (1968)\n"
	"                \n"
	"                \n"
	"                    \n"
	"                        \n"
	"                                                        \n"
	"                                Prize\n"
	"                                Claimed\n"
	"                                Remaining\n"
	"                            \n"
	"              \\t                                              																									                                \n"
	"                                    $1,000\n"
	"                                    3\n"
	"                                    15\n"
	"                                \n"
	"                                                            																									                                \n"
	"                                    $30,000\n"
	"                                    2\n"
	"                                    5\n"
	"                                \n"
	"                                                    \n"
	"                \n"
	"                \n"
	"            ")

subst = "\\n"

# You can manually specify the number of replacements by changing the 4th argument
result = re.sub(regex, subst, test_str, 0, re.MULTILINE)

if result:
    print (result)

