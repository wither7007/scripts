ws('pascal')
ws('Pascal')
ws('Bacon')
def ws(x):
    print(x)
    ret=wikipedia.summary(x)
    return ret
ws('Bacon')
ws('Francis Bacon')
wss('Pascal')
ws('Blaise Bacon')
def wss(x):
  print(x)
  res=wikipedia.search(x)
  rend(res)
  return res
ws('Blaise Bacon')
wss('Pascal')
ws('Blaise Pascal')
