def ns (s):
    regex = r"\s{2,}"
    subst = "\\n"
    s=s.strip().replace('\nPrize\nClaimed\nRemaining\n','') 
    result = re.sub(regex, subst, s, 0, re.MULTILINE)
    result=result+'xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx'
    return result

