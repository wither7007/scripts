def pConv(pat='/mnt/'):
    if len(sys.argv)>1:
        aa=sys.argv[1]
    # pdb.set_trace()
    pats = re.search("/mnt/c/", pat)
    if pats:
        bb=pat.replace('/mnt/c/','c:\\').replace('/', '\\')
        print(f"{pat} is {bb}")

        # print("not linupatx")
    patx = re.search(r"c:\\", pat)
    if patx:
        # print('windows')
        print(pat)
        winL(pat)
